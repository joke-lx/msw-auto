{
  "name": "msw-auto",
  "version": "2.12.13",
  "description": "Intelligent automated Mock server with Web UI and AI generation capabilities",
  "type": "module",
  "bin": {
    "msw-auto": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:cli\" \"pnpm dev:server\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:cli": "node --watch cli/index.js",
    "clean": "rimraf ./dist",
    "build": "pnpm clean && tsup --config tsup.config.simple.ts",
    "test": "vitest",
    "server": "tsx src/server/index.ts",
    "cli": "node cli/index.js",
    "mcp": "tsx src/mcp/server.ts"
  },
  "homepage": "https://msw-auto.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msw-auto/msw-auto.git"
  },
  "author": "MSW Auto Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@inquirer/confirm": "^5.0.0",
    "@inquirer/input": "^5.0.0",
    "@inquirer/select": "^5.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@mswjs/interceptors": "^0.41.2",
    "better-sqlite3": "^11.8.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "clui": "^0.3.6",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "ora": "^6.3.1",
    "picocolors": "^1.1.1",
    "ws": "^8.18.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@inquirer/prompts": "^8.3.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/node": "~20.19.25",
    "@types/ws": "^8.18.0",
    "@types/yargs": "^17.0.33",
    "babel-minify": "^0.5.2",
    "concurrently": "^9.0.0",
    "esbuild": "^0.27.0",
    "glob": "^13.0.6",
    "outvariant": "^1.4.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
