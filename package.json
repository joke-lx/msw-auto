{
  "name": "msw-auto",
  "version": "2.12.10",
  "description": "Intelligent automated Mock server with Web UI and AI generation capabilities",
  "type": "module",
  "main": "./lib/core/index.js",
  "module": "./lib/core/index.mjs",
  "types": "./lib/core/index.d.ts",
  "packageManager": "pnpm@9.15.0",
  "exports": {
    ".": {
      "module-sync": {
        "types": "./lib/core/index.d.mts",
        "default": "./lib/core/index.mjs"
      },
      "module": {
        "types": "./lib/core/index.d.mts",
        "default": "./lib/core/index.mjs"
      },
      "import": {
        "types": "./lib/core/index.d.mts",
        "default": "./lib/core/index.mjs"
      },
      "default": {
        "types": "./lib/core/index.d.ts",
        "default": "./lib/core/index.js"
      }
    },
    "./mockServiceWorker.js": "./lib/mockServiceWorker.js",
    "./server": "./src/server/index.ts",
    "./mcp": "./src/mcp/server.ts",
    "./package.json": "./package.json"
  },
  "bin": {
    "msw": "cli/index.js",
    "msw-auto": "cli/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:core\" \"pnpm dev:web\"",
    "dev:core": "tsup --watch",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:web": "pnpm --filter web dev",
    "dev:cli": "node --watch cli/index.js",
    "clean": "rimraf ./lib",
    "lint": "eslint \"{cli,src}/**/*.ts\"",
    "build": "pnpm clean && tsup",
    "build:web": "pnpm --filter web build",
    "build:cli": "tsc cli/*.ts --outDir dist-cli",
    "test": "vitest",
    "server": "tsx src/server/index.ts",
    "cli": "node cli/index.js",
    "mcp": "tsx src/mcp/server.ts"
  },
  "homepage": "https://msw-auto.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msw-auto/msw-auto.git"
  },
  "author": "MSW Auto Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@inquirer/confirm": "^5.0.0",
    "@inquirer/input": "^5.0.0",
    "@inquirer/select": "^5.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@mswjs/interceptors": "^0.41.2",
    "better-sqlite3": "^11.8.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "clui": "^0.3.6",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "ora": "^6.3.1",
    "picocolors": "^1.1.1",
    "ws": "^8.18.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@inquirer/prompts": "^8.3.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/node": "~20.19.25",
    "@types/ws": "^8.18.0",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.0.0",
    "esbuild": "^0.27.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
